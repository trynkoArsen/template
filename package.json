{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "analyze": "ANALYZE_BUNDLE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx pages src",
    "postinstall": "husky install",
    "setup:husky": "rimraf .husky/pre-commit .husky/pre-push && husky add .husky/pre-commit \"npx lint-staged\" && husky add .husky/pre-push \"npx tsc --noEmit\"",
    "setup": "yarn setup:husky && cpy --rename=.env example.env ./"
  },
  "dependencies": {
    "@devimasolutions/auth": "^0.3.2",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.13.7",
    "@mui/styles": "^5.11.7",
    "@reduxjs/toolkit": "^1.9.2",
    "@sentry/nextjs": "^7.36.0",
    "axios": "0.27.2",
    "formik": "^2.2.9",
    "next": "^13.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-toastify": "^9.1.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.6",
    "@sentry/cli": "^2.12.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{css,scss,ts,tsx,html}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
